class Solution {
  public:
    int search(vector<int>& arr, int key) {
        int ind=0,i;
        for(i=0;i<arr.size();i++){
            if(arr[i]==key){
                ind++;
                break;
            }
        }if(ind==0){
            return -1;
        }else{
            return i;
        }
    }
};
